package classes.asset.land{	import classes.asset.land.Land;		import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.MouseEvent;		public class Australia extends Land	{		public const MAX:uint = 5;						public var papuaNewGuinea:MovieClip;		public var australia:MovieClip;		public var newZealand:MovieClip;		public var fiji:MovieClip;		public var solomonIslands:MovieClip;				public var countrys:Array;				private const _X:Number = 52;		private const _Y:Number = 50;		private const _SCALE = 1;						public function Australia(bool:Boolean=false)		{			super(5, "Australia", _X, _Y, _SCALE, MAX);			//trace("Australia")			if(bool){				SetName();				SetAry();			}		}				private function SetAry():void		{			countrys = [				papuaNewGuinea,				australia,				newZealand,				fiji,				solomonIslands			];					shuffle(countrys);			InitColor(countrys);					}				private function SetName(){			papuaNewGuinea._en = "Papua New Guinea";			papuaNewGuinea._ja = "パプアニューギニア";			australia._en = "Australia";			australia._ja = "オーストラリア";			newZealand._en = "New Zealand";			newZealand._ja = "ニュージーランド";			fiji._en = "Fiji";			fiji._ja = "フィジー";			solomonIslands._en = "Solomon Islands";			solomonIslands._ja = "ソロモン諸島";		}	}}